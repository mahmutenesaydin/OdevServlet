<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>enes</servlet-name>
    <servlet-class>uniyazservlet.Servlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>dogetList</servlet-name>
    <servlet-class>uniyazservlet.doGetList</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>enes</servlet-name>
    <url-pattern>/yaz</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>dogetList</servlet-name>
    <url-pattern>/listele</url-pattern>
  </servlet-mapping>
</web-app>